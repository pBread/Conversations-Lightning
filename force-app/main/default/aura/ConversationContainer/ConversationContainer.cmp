<aura:component controller="TwilioConversationApi">
  <aura:handler name="init" value="{!this}" action="{!c.init}" />
  <aura:attribute name="conversationSids" type="String[]" />

  <div class="c-header">
    <img height="50" width="50" src="{!$Resource.twilio_logo}" />
  </div>

  <aura:iteration
    items="{!v.conversationSids}"
    var="conversationSid"
    indexVar="key"
  >
    {!conversationSid}<br /><br />
  </aura:iteration>
</aura:component>
